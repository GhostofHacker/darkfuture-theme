	"tokenColors": [
		{
			"name": "Punctuations",
			"scope": [
				"punctuation",
				"storage.type.function.arrow",
				"keyword.operator.increment",
				"keyword.operator.relational",
			],
			"settings": {
				"foreground": "#D1DFE5"
			}
		},
		{
			"name": "Second Type Punctuations",
			"scope": [
				"punctuation.definition.parameters",
				"punctuation.definition.string",
			],
			"settings": {
				"foreground": "#dfde9d",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#477925"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#89c4fc"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other",
			],
			"settings": {
				"foreground": "#bf89fc",
				"fontStyle": "italic",
			}
		},
		{
			"name": "Variable Declerations",
